$(document).ready(function(){$("[data-collapsed='false'] ul").css("display","block"),$("[data-collapsed='false']").toggleClass("active"),$("[data-collapsed='true'] ul").css("display","none"),$("[data-collapsed] header").on("click",function(){var a=$(this).parent(),l=a.children("ul"),e=a.attr("data-collapsed");"true"===e?(l.slideDown(),a.attr("data-collapsed","false"),a.toggleClass("active")):(l.slideUp(),a.attr("data-collapsed","true"),a.toggleClass("active"))})});