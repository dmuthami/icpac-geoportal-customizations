$(document).ready(function(){$("#helpSearchForm, #helpSearchFormTop").submit(function(){var e=".site-search #helpSearchForm";$(".drawer").is(":visible")&&(e=".drawer #helpSearchForm");var n=$(e+" > input[name='q']").val()||"",o=$(e+" > input[name='collection']").val()||"",a=$(e+" > input[name='product']").val()||"",t=$(e+" > input[name='language']").val()||"en",i=$(e+" > input[name='platform']").val()||"",c=$(e+" > input[name='solution']").val()||"",r=$("#helpSearchForm > input[name='subject']").val()||"",p="/search/?";if("detect"===i){var l=navigator.userAgent,m=l.match(/(iPhone|iPod|iPad)/ig),d=(l.match(/(Android)/ig),l.match(/(Windows Phone)/ig));"collector-android"===a?m&&(a="collector-ios"):"android-app"===a&&(m?a="ios-app":d&&(a="win-phone-app"))}return p=p+"q="+encodeURIComponent(n),p=p+"&collection="+encodeURIComponent(o),a&&(p=p+"&product="+encodeURIComponent(a)),c&&(p=p+"&solution="+encodeURIComponent(c)),r&&(p=p+"&subject="+encodeURIComponent(r)),p=p+"&language="+encodeURIComponent(t),window.location.href=p,!1})});