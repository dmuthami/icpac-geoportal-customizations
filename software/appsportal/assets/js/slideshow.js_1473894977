$(document).ready(function(){$(".slideshow").on("cycle-post-initialize",function(e){$(this).children(".slide").length<=1&&($(this).children(".cycle-pager").css("visibility","hidden"),$(this).children(".prev").css("display","none"),$(this).children(".next").css("display","none"),e.stopPropagation())}),$(".slideshow").cycle({timeout:0,manualSpeed:200,slides:"> .slide",pager:"> .cycle-pager",prev:"> .prev",next:"> .next",allowWrap:!1})});