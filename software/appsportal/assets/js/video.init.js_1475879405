$(document).ready(function(){function e(e,t,i){document.getElementById(e).play(),$("#"+e).bind("ended",function(){$("#"+e).css("display","none"),$(i).css("display","block"),$(i).attr("href",t)})}function t(e,t){var i=document.createElement("source");i.src="http://video.arcgis.com/download-video/"+e+"/mp4/480",i.type="video/mp4";var n=document.createElement("video");n.appendChild(i),n.setAttribute("height","240"),n.setAttribute("id",e),n.setAttribute("width","320"),$(t).after(n)}$(".videoLink").colorbox({iframe:!0,innerWidth:960,innerHeight:540,scrolling:!1}),navigator.userAgent.match(/(iPhone|iPod|iPad|Android|blackberry)/ig)&&($("a.cboxElement").removeClass("cboxElement"),$(".videoLink").click(function(){var i=$(this).attr("href"),n=i.split("/"),o=n[4];$(this).attr("href","javascript:void(0)"),$(this).css("display","none"),0==$("#"+o).length?t(o,this):$("#"+o).css("display","block"),e(o,i,this)}))});